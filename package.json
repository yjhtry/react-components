{
  "name": "@yjhtry/react-components",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@9.0.6",
  "description": "react component library",
  "license": "MIT",
  "homepage": "https://github.com/yjhtry/react-components#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjhtry/react-components.git"
  },
  "bugs": {
    "url": "https://github.com/yjhtry/yjhtry/react-components/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "release": "pnpm build && bumpp && pnpm publish --no-git-checks",
    "typecheck": "tsc",
    "test": "vitest"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.1",
    "@types/node": "^20.12.7",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@yelo/rollup-node-external": "^1.0.1",
    "bumpp": "^9.4.1",
    "eslint": "^9.2.0",
    "eslint-plugin-format": "^0.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "nprogress": "^0.2.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
